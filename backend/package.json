{
  "name": "echoprompt-backend",
  "version": "1.0.0",
  "description": "Backend API for EchoPrompt - AI Prompt Generator",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "tsx watch src/server.ts",
    "dev:mock": "tsx watch src/server-mock.ts",
    "build": "tsc",
    "build:mock": "tsc src/server-mock.ts --outDir dist --esModuleInterop --skipLibCheck",
    "start:mock": "node dist/server-mock.js",
    "seed": "tsx src/scripts/seedDatabase.ts",
    "lint": "eslint src/**/*.ts",
    "test": "jest"
  },
  "keywords": [
    "ai",
    "prompt",
    "generator",
    "api"
  ],
  "author": "EchoPrompt",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@types/mongoose": "^5.11.96",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.18.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "openai": "^4.20.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.4",
    "@types/nodemailer": "^6.4.14",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  }
}
